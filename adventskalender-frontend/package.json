{
  "name": "adventskalender-frontend",
  "version": "2021.11.5",
  "private": true,
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "jsonwebtoken": "^8.5.1",
    "@mui/material": "^5.0.6",
    "@mui/lab": "^5.0.0-alpha.53",
    "@mui/icons-material": "^5.0.5",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "react-router-dom": "6.0.0",
    "react-router": "6.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.36",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-test-renderer": "^17.0.1",
    "react-test-renderer": "^17.0.2",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "tslint": "^6.1.3",
    "tslint-config-airbnb-base": "^0.3.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "check-format": "prettier --list-different \"{{public,src,scripts,types},.}/**/*.{js,jsx,ts,tsx,md,scss,css,json,htm,html}\" .prettierrc",
    "format": "prettier --write \"{{public,src,scripts,types},.}/**/*.{js,jsx,ts,tsx,md,scss,css,json,htm,html}\" .prettierrc",
    "lint": "tslint --format stylish {src,scripts}/**/*.{ts,tsx}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
