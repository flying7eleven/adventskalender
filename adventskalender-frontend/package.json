{
    "name": "adventskalender-frontend",
    "version": "2024.11.18",
    "private": true,
    "dependencies": {
        "@emotion/babel-plugin": "^11.12.0",
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@mui/base": "^5.0.0-beta.61",
        "@mui/icons-material": "^6.1.7",
        "@mui/lab": "^6.0.0-beta.15",
